{
  "name": "mcswiss",
  "author": "Liam Codes",
  "description": "A video editing toolbox",
  "private": true,
  "version": "0.2.3",
  "versionReleased": "2024-05-29",
  "type": "module",
  "main": "./electron.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "electron:dev": "npm run dev & electron ./electron.cjs",
    "electron:build": "npm run build && electron-builder",
    "postinstall": "node scripts/patch-transformers-exports.cjs",
    "publish:win": "electron-builder --win --x64 -p always",
    "publish:mac": "electron-builder --mac --arm64 -p always",
    "publish": "electron-builder -p always"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LiammCodes/McSwissVue"
  },
  "dependencies": {
    "@electron/remote": "^2.0.11",
    "@ffmpeg/core": "^0.11.0",
    "@ffmpeg/ffmpeg": "^0.11.6",
    "@ffmpeg/util": "^0.12.1",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@heroicons/vue": "^2.0.17",
    "@huggingface/transformers": "^3.8.1",
    "@vueuse/core": "^10.4.1",
    "api": "^6.1.1",
    "app-root-dir": "^1.0.2",
    "aws-sdk": "^2.1593.0",
    "child_process": "^1.0.2",
    "custom-electron-titlebar": "^4.2.8",
    "electron-is-packaged": "^1.0.2",
    "electron-root-path": "^1.1.0",
    "electron-updater": "^6.1.8",
    "ffmpeg-static": "^5.1.0",
    "pinia": "^2.0.34",
    "rollup-plugin-node-builtins": "^2.1.2",
    "secure-ls": "^1.2.6",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vue-the-mask": "^0.11.1",
    "vue-toasted": "^1.1.28",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.21",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.16",
    "daisyui": "^4.9.0",
    "electron": "^24.0.0",
    "electron-builder": "^23.6.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.4.3",
    "typescript": "^4.9.3",
    "vite": "^5.2.6",
    "vue-tsc": "^1.0.11"
  },
  "build": {
    "appId": "com.liamcodes.mcswiss",
    "productName": "McSwiss",
    "directories": {
      "output": "build"
    },
    "extraMetadata": {
      "main": "./electron.cjs"
    },
    "extraFiles": [
      {
        "from": "resources/mac/bin",
        "to": "Resources/bin",
        "filter": [
          "**/*"
        ]
      }
    ],
    "extends": null,
    "files": [
      "dist/**/*",
      "preload.ts",
      "electron.cjs"
    ],
    "asarUnpack": [
      "node_modules/@huggingface/transformers/**",
      "node_modules/onnxruntime-node/**"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "LiammCodes",
        "repo": "McSwissVue"
      }
    ]
  }
}
